{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_DEBUG": "0",
    "FLASK_ENV": "production"
  },
  "buildCommand": "pip install -r requirements.txt && python vercel_setup.py"
}